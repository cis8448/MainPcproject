<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pcproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".login">
            <intent-filter>
                <action android:name="com.example.pcproject.login"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>

        <activity android:name=".myinfo">
            <intent-filter>
                <action android:name="com.example.pcproject.myinfo"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name=".memberadd">
            <intent-filter>
                <action android:name="com.example.pcproject.memberadd"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name=".seatdata">
            <intent-filter>
                <action android:name="com.example.pcproject.seatdata"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name=".membermanagment">
            <intent-filter>
                <action android:name="com.example.pcproject.membermanagment"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name=".productmanagment">
            <intent-filter>
                <action android:name="com.example.pcproject.productmanagment"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name=".seatmanager">
            <intent-filter>
                <action android:name="com.example.pcproject.seatmanager"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name=".productadd">
            <intent-filter>
                <action android:name="com.example.pcproject.productadd"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name=".photoadd">
            <intent-filter>
                <action android:name="com.example.pcproject.photoadd"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name=".Productorder">
            <intent-filter>
                <action android:name="com.example.pcproject.productorder"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>


    </application>

</manifest>